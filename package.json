{
  "name": "skeleton-storybook-vue",
  "version": "2.0.0",
  "description": "The skeleton of StoryBook UI for Vue.js",
  "homepage": "https://github.com/mornya/skeleton-storybook-vue",
  "bugs": "https://github.com/mornya/skeleton-storybook-vue/issues",
  "keywords": [
    "mornya",
    "skeleton",
    "storybook",
    "vue.js",
    "vue3",
    "frontend"
  ],
  "author": "mornya <mornya78@gmail.com>",
  "repository": "https://github.com/mornya/skeleton-storybook-vue.git",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "husky || exit 0",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier --write --ignore-path .prettierignore ./.storybook ./src ./types",
    "check": "tsc --noEmit && vue-tsc --noEmit",
    "lint": "eslint",
    "test": "vitest",
    "sonar": "sonar-scanner"
  },
  "type": "module",
  "engines": {
    "node": ">=20.11 || >=22",
    "npm": ">=6"
  },
  "dependencies": {
    "storybook": "^9.0.17",
    "vue": "^3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/js": "^9.29.0",
    "@storybook/addon-a11y": "^9",
    "@storybook/addon-docs": "^9",
    "@storybook/addon-onboarding": "^9",
    "@storybook/addon-vitest": "^9",
    "@storybook/vue3-vite": "^9",
    "@types/node": "^24.0.15",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/reactivity": "^3.5.17",
    "@vue/runtime-core": "^3.5.17",
    "eslint": "^9",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-storybook": "^9.0.17",
    "eslint-plugin-unicorn": "^60.0.0",
    "eslint-plugin-vue": "^10.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "playwright": "^1.54.1",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "6.3.5",
    "vite-plugin-vue-devtools": "^7",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.3"
  },
  "resolutions": {
    "vite": "6.3.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "lint-staged": {
    "*.{css,scss}": [
      "prettier --write --ignore-path .prettierignore"
    ],
    "*.{ts,tsx,vue}": [
      "prettier --write --ignore-path .prettierignore",
      "lintest check",
      "lintest lint"
    ]
  }
}

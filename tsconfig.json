{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES5",
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@storybook/vue",
      "./types"
    ],
    "types": [
      "jest",
      "node",
      "webpack-env"
    ],
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "paths": {
      "~/*": ["./src/*"],
      "@/*": ["./src/*"]
    },
    "charset": "utf8",
    "baseUrl": ".",
    "outDir": "./dist",
    "jsx": "preserve",
    "importsNotUsedAsValues": "preserve",
    "declaration": false,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": false,
    "strict": true,
    "sourceMap": false,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true,
    "noEmit": true,
    "isolatedModules": true
  },
  "include": [
    "types/**/*",
    "src/**/*"
  ],
  "exclude": [
    ".idea",
    ".storybook",
    "dist",
    "node_modules"
  ]
}
